{
    "name": "komfysach/laravel-graphql-generator",
    "description": "GraphQL schema generator for Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "KomfySach",
            "email": "sachinlendis@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0|^10.0",
        "rebing/graphql-laravel": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "KomfySach\\LaravelGraphQLGenerator\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "KomfySach\\LaravelGraphQLGenerator\\GraphQLGeneratorServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
